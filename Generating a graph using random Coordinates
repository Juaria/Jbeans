import random 
import matplotlib.pyplot as plt 
import numpy as np 

# Generating a graph using random coordinates 
x = np.arange(1, 10, 5) 
y = np.arange(10, 20, 5)
 
plt.plot(x, y) 

  
# naming the x axis 
plt.xlabel('x - axis') 
# naming the y axis 
plt.ylabel('y - axis') 
# giving a title
plt.title('Graph') 
  
# function to show the plot 
plt.show() 
